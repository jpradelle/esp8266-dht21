    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "extract-loader": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "lit-scss-loader": "^2.0.1",
    "lit-web-types": "^0.1.5",
    "material-components-web-types": "^0.2.2",
    "mock-json-server": "^1.3.6",
    "node-fetch": "^3.3.0",
    "paper-elements-web-types": "^0.2.1",
