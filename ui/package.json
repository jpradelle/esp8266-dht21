{
  "name": "haTemp",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "dev-mocked": "API_ADDRESS='http://127.0.0.1:3000' yarn dev",
